<!-- /Block Demo -->
  <script>
    $(document).ready(function(){
    
      $('.bx-switcher').each(function (e) {
        var classCol = "";
        
        $(this).find("option").each(function (e) {
          var val = $(this).attr("value");
          if(val) {
            classCol += val + " ";
          }
        });
      
        $(this).attr("data-remove", classCol);
      });
        
      $('.bx-switcher').change(function (e) {

        var val = $(this).val();
        var rmv = $(this).attr("data-remove");
        $("#bx-demo-class").html("my-slider " + val);
        $("#bx-demo").removeClass(rmv).addClass(val);
        var cval = "\"" + $("#bx-demo").attr("class") + "\"";
        $("#bx-demo-class").html(cval);

      });

    });
  </script>