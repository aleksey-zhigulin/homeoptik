<h6>Stacked Checkboxes & Radios</h6>
<form>    
  <div class="row">
    <div class="col-md-4">
      <div class="checkbox"><label><input type="checkbox" value="">Mercury</label></div>
      <div class="checkbox"><label><input type="checkbox" value="">Venus</label></div>
      <div class="checkbox"><label><input type="checkbox" value="">Earth</label></div>
    </div>
    <div class="col-md-4">
      <div class="checkbox"><label><input type="checkbox" value="">Mars</label></div>
      <div class="checkbox"><label><input type="checkbox" value="">Just</label></div>
      <div class="checkbox"><label><input type="checkbox" value="">Saturn</label></div>
    </div>
    <div class="col-md-4">
      <div class="radio"><label><input type="radio" name="demoRadio1" value="" >Uranus</label></div>
      <div class="radio"><label><input type="radio" name="demoRadio1" value="">Neptune</label></div>
      <div class="radio"><label><input type="radio" name="demoRadio1" value="">Pluto</label></div>
    </div>
  </div>
</form>

<h6>Inline Checkboxes & Radios</h6>
<form>
  <div class="row">
    <div class="col-md-6">
      <label class="checkbox-inline"><input type="checkbox" value="">Mercury</label>
      <label class="checkbox-inline"><input type="checkbox" value="">Venus</label>
      <label class="checkbox-inline"><input type="checkbox" value="">Earth</label>
    </div>
    <div class="col-md-6">
      <label class="radio-inline"><input type="radio" name="demoRadio2" value="">Uranus</label>
      <label class="radio-inline"><input type="radio" name="demoRadio2" value="">Neptune</label>
      <label class="radio-inline"><input type="radio" name="demoRadio2" value="">Pluto</label>
    </div>
  </div>
</form>
