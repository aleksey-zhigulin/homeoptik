<script>
  $().ready(function() {
    /* 
    =======================================================
      Adapted from >> http://daneden.github.io/animate.css/ 
    =======================================================
    */
    
    $("#anim-select").change(function() {
      var val = $(this).val();
      testAnim(val);
    });
    
    $("#replay").click(function() {
      var val = $("#anim-select").val();
      testAnim(val);
    });
    
    function testAnim(x) {
      $('#anim-object').removeClass().addClass(x + ' animated').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function(){
      $(this).removeClass();
      });
    }; 
  
  });
</script>